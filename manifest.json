  "related_applications": [],
  "prefer_related_applications": false,
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "minimal-ui"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+iautomatize",
      "url": "/blog.html?article=%s"
    }
  ],
  "share_target": {
    "action": "/share-article",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "text/plain": [".txt"],
        "text/markdown": [".md"],
        "application/json": [".json"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "scope_extensions": [
    {
      "origin": "https://iautomatize.com"
    }
  ]
}